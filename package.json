{
  "name": "shopping-cart",
  "version": "0.0.0",
  "scripts": {
    "coverage": "cross-env NODE_ENV=test nyc mocha",
    "e2e": "nightwatch",
    "ci": "npm ci && lerna exec --stream -- npm ci",
    "test": "cross-env NODE_ENV=test mocha",
    "cucumber": "cross-env NIGHTWATCH_ENV=chrome cucumber-js tests/cucumber/features/**/*.feature --require @babel/register --require cucumber.conf.js --require tests/cucumber/step-definitions --format node_modules/cucumber-pretty --format json:.reports/cucumber/report.json",
    "format": "prettier --check {src,tests}/**",
    "lint": "eslint {src,tests}/**/*.{html,js,json,jsx,ts,tsx,gql,graphql}",
    "ncu": "ncu --dep prod,dev -t minor -u",
    "ncu:preview": "ncu --dep prod,dev",
    "reinstall": "npm run remove && npm install",
    "reinstall:openapi": "npm cache clear -f && rm -rf node_modules/@modevel package-lock.json && npm i",
    "remove": "rm -rf node_modules package-lock.json",
    "update": "ncu -t minor -u && npm install && ncu",
    "webpack:build": "cross-env NODE_ENV=production webpack",
    "webpack:serve": "cross-env NODE_ENV=development webpack serve --devtool=source-map",
    "webpack:start": "npm run webpack:serve -- --open=chrome",
    "build": "rm -rf dist && npm run webpack:build",
    "preview": "vite preview",
    "serve": "npm run vite:serve",
    "start": "npm run webpack:start",
    "prepare": "husky install",
    "commit-msg": "commitlint",
    "pre-commit": "lint-staged"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "12.2.0",
    "@angular/compiler": "12.2.0",
    "@angular/compiler-cli": "12.2.0",
    "@angular/core": "12.2.0",
    "@angular/forms": "12.2.0",
    "@angular/platform-browser": "12.2.0",
    "@angular/platform-browser-dynamic": "12.2.0",
    "@leanup/lib": "1.2.0-rc.101",
    "core-js": "3.16.0",
    "navigo": "8.11.1",
    "preact": "10.5.14",
    "register-service-worker": "1.7.2",
    "rxjs": "7.3.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@leanup/cli-cucumber": "1.2.0-rc.101",
    "@leanup/cli-pwa": "1.2.0-rc.101",
    "@leanup/git-hooks": "1.2.0-rc.99",
    "@leanup/stack": "1.2.0-rc.101",
    "@leanup/stack-angular": "1.2.0-rc.101",
    "@leanup/stack-preact": "1.2.0-rc.101",
    "@leanup/stack-webpack": "1.2.0-rc.101",
    "@ngtools/webpack": "12.2.0",
    "chromedriver": "92.0.1",
    "navigo": "8.11.1",
    "npm-check-updates": "11.8.3",
    "selenium-server": "3.141.59",
    "typescript": "4.3.5",
    "workbox-cli": "6.2.2"
  },
  "lint-staged": {
    "{src,tests}/**/*.{html,js,json,jsx,ts,tsx,gql,graphql}": [
      "eslint"
    ],
    "{src,tests}/**": [
      "prettier --check"
    ]
  }
}
